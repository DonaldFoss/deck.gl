{
  "name": "layer-browser",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "rollupify",
        {
          "config": "rollup.config.js"
        }
      ]
    ]
  },
  "scripts": {
    "watch": "rollup -c rollup.config.js app.js -o dist/app.js",
    "build": "mkdir -p dist && browserify app.js -o dist/app.js",
    "start": "budo app.js --open"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "deck.gl": "^3.1.1",
    "luma.gl": "^2.10.3",
    "react": "^15.4.1",
    "react-autobind": "^1.0.6",
    "react-dat-gui": "0.0.13",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-stats": "0.0.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "buble": "^0.15.1",
    "bubleify": "^0.7.0",
    "budo": "^9.2.2",
    "jsx-transform": "^2.3.0",
    "reactify": "^1.1.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^6.0.1",
    "rollup-plugin-hypothetical": "^1.2.1",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-serve": "^0.1.0",
    "rollupify": "^0.3.7"
  }
}
